{
    "name": "readiness-backend",
    "version": "1.0.0",
    "description": "Secure backend for Military Readiness App - Accepts ONLY calculated scores",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "migrate": "ts-node src/database/migrate.ts",
        "lint": "eslint src --ext .ts",
        "test": "jest"
    },
    "keywords": [
        "readiness",
        "health",
        "scores",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.1.5",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.3.1",
        "pg": "^8.11.3",
        "joi": "^17.11.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/bcryptjs": "^2.4.6",
        "@types/pg": "^8.10.9",
        "@types/node": "^20.10.6",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3",
        "eslint": "^8.56.0",
        "@typescript-eslint/parser": "^6.16.0",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "jest": "^29.7.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}